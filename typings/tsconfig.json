{
  "compilerOptions": {
    "module": "commonjs",
    "noEmit": true,
    "lib": ["es6"],
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "forceConsistentCasingInFileNames": true,
    "experimentalDecorators": true,
    "noImplicitAny": true,
    "noImplicitThis": true,
    "strictNullChecks": true,
    "strictFunctionTypes": true,
    "baseUrl": "./",
    "paths": { "mangopay2-nodejs-sdk": ["./"] },
    "types": [],
    "typeRoots": ["../"],
    "skipLibCheck": true
  }
}
